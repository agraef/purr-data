install-pdexecPROGRAMS: $(pdexec_PROGRAMS)
	@$(NORMAL_INSTALL)
	test -z "$(pdexecdir)" || $(mkdir_p) "$(DESTDIR)$(pdexecdir)"
	@list='$(pdexec_PROGRAMS)'; for p in $$list; do \
	  p1=`echo $$p|sed 's/$(EXEEXT)$$//'`; \
	  if test -f $$p \
	  ; then \
	    f=`echo "$$p1" | sed 's,^.*/,,;$(transform);s/$$/$(EXEEXT)/'`; \
	   echo " $(INSTALL_PROGRAM_ENV) $(pdexecPROGRAMS_INSTALL) '$$p' '$(DESTDIR)$(pdexecdir)/$$f'"; \
	   $(INSTALL_PROGRAM_ENV) $(pdexecPROGRAMS_INSTALL) "$$p" "$(DESTDIR)$(pdexecdir)/$$f" || exit 1; \
	  else :; fi; \
	done

uninstall-pdexecPROGRAMS:
	@$(NORMAL_UNINSTALL)
	@list='$(pdexec_PROGRAMS)'; for p in $$list; do \
	  f=`echo "$$p" | sed 's,^.*/,,;s/$(EXEEXT)$$//;$(transform);s/$$/$(EXEEXT)/'`; \
	  echo " rm -f '$(DESTDIR)$(pdexecdir)/$$f'"; \
	  rm -f "$(DESTDIR)$(pdexecdir)/$$f"; \
	done

